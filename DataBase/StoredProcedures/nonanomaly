-- Begin Create

DELIMITER $$
DROP PROCEDURE IF EXISTS CreateNonanomaly $$
CREATE PROCEDURE CreateNonanomaly(
     IN IpSource varchar(45),
     IN IpDestination varchar(45),
     IN PortSource int,
     IN PortDestination int
) BEGIN

INSERT INTO NONANOMALY (IpSource,IpDestination,PortSource,PortDestination)
VALUES (IpSource,IpDestination,PortSource,PortDestination);

END $$
DELIMITER ;

-- CALL CreateNonanomaly('string''string'5353);
-- End Create
-- Begin Get all

DELIMITER $$
DROP PROCEDURE IF EXISTS GetAllNonanomaly $$
CREATE PROCEDURE GetAllNonanomaly(
) BEGIN

SELECT * FROM NONANOMALY;
END $$
DELIMITER ;

-- CALL GetAllNonanomaly();
-- End Get All
-- Begin Get

DELIMITER $$
DROP PROCEDURE IF EXISTS GetNonanomaly $$
CREATE PROCEDURE GetNonanomaly(
IN ID int
) BEGIN

SELECT * FROM NONANOMALY WHERE NONANOMALY.id = ID;
END $$
DELIMITER ;

-- CALL GetNonanomaly(1);
-- End Get
-- Begin Update

DELIMITER $$
DROP PROCEDURE IF EXISTS UpdateNonanomaly $$
CREATE PROCEDURE UpdateNonanomaly(
     IN IpSource varchar(45),
     IN IpDestination varchar(45),
     IN PortSource int,
     IN PortDestination int,
     IN ID int
) BEGIN

UPDATE NONANOMALY SET IpSource = IpSource,IpDestination = IpDestination,PortSource = PortSource,PortDestination = PortDestination
WHERE NONANOMALY.ID=ID;
END $$
DELIMITER ;

-- CALL UpdateNonanomaly(1,'string''string'5353);
-- End Create


-- Begin Delete

DELIMITER $$
DROP PROCEDURE IF EXISTS DeleteNonanomaly $$
CREATE PROCEDURE DeleteNonanomaly(
IN ID int
) BEGIN

DELETE FROM NONANOMALY WHERE NONANOMALY.id = ID;
END $$
DELIMITER ;

-- CALL DeleteNonanomaly(1);
-- End Delete
